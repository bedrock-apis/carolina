{
   "name": "@bedrock-apis/carolina-net",
   "version": "1.0.0-beta.1",
   "type": "module",
   "keywords": [
      "minecraft",
      "raknet",
      "bedrock",
      "protocol"
   ],
   "author": "bedrock-apis",
   "license": "MIT",
   "scripts": {
      "check": "tsgo --noEmit",
      "build": "rolldown -c ../../rolldown.common.config.ts",
      "watch": "rolldown -c ../../rolldown.common.config.ts -w"
   },
   "exports": {
      ".": {
         "default": "./dist/main.js",
         "types": "./dist/main.d.ts"
      },
      "./raknet": {
         "default": "./dist/raknet.js",
         "types": "./dist/raknet.d.ts"
      }
   },
   "rolldown": {
      "entries": {
         "main": "./api/interface.ts",
         "raknet": "./raknet/main.ts",
         "nethernet": "./nethernet/main.ts"
      },
      "dir": "./dist",
      "external": "dependencies",
      "declarations": true
   }
}