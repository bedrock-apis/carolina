{
   "name": "@bedrock-apis/carolina-authentication",
   "version": "1.0.0-beta.2",
   "type": "module",
   "keywords": [
      "minecraft",
      "protocol",
      "auth",
      "validation",
      "bedrock"
   ],
   "scripts": {
      "check": "tsgo --noEmit",
      "build": "rolldown -c ../../rolldown.common.config.ts",
      "watch": "rolldown -c ../../rolldown.common.config.ts -w"
   },
   "main": "./dist/main.js",
   "types": "./dist/main.d.ts",
   "author": "bedrock-apis",
   "license": "MIT",
   "files": [
      "README.md",
      "package.json",
      "./dist/*"
   ],
   "rolldown": {
      "entries": {
         "main": "./src/main.ts"
      },
      "dir": "./dist",
      "external": "dependencies",
      "declarations": true,
      "sourcemap": false
   }
}