{
   "name": "@carolina/core",
   "version": "1.0.0",
   "type": "module",
   "main": "dist/main.js",
   "--comment--": "We use src as we can link the files the typescript loads and rename works across",
   "types": "src/main.ts",
   "keywords": [],
   "author": "bedrock-apis",
   "license": "MIT",
   "scripts": {
      "build": "rolldown -c ./rolldown.config.ts",
      "check": "tsgo --noEmit",
      "watch": "rolldown -w -c"
   },
   "dependencies": {
      "@carolina/binary": "workspace:^",
      "@carolina/protocol": "workspace:^",
      "@bedrock-apis/carolina-net": "workspace:^",
      "@types/bun": "^1.2.20"
   },
   "rolldown": {
      "entries": {
         "main": "./src/main.ts",
         "thread": "./src/thread.ts"
      },
      "dir": "./dist",
      "external": "dependencies",
      "declarations": true,
      "mappings": true
   }
}