{
   "name": "@carolina/dumper",
   "version": "1.0.0",
   "type": "module",
   "main": "dist/main.js",
   "--comment--": "We use src as we can link the files the typescript loads and rename works across",
   "types": "src/main.ts",
   "keywords": [],
   "author": "bedrock-apis",
   "license": "MIT",
   "scripts": {
      "check": "tsgo --noEmit",
      "build": "rolldown -c ../../rolldown.common.config.ts",
      "watch": "rolldown -c ../../rolldown.common.config.ts -w"
   },
   "devDependencies": {
      "@minecraft/server": "2.3.0-beta.1.21.110-preview.26",
      "@minecraft/server-net": "1.0.0-beta.1.21.110-preview.26",
      "@types/bun": "^1.3.2",
      "@types/node": "^24.0.0",
      "unzip-web-stream": "0.0.0-alpha.1"
   },
   "rolldown": {
      "entries": {
         "main": "./src/main.ts"
      },
      "dir": "./dist",
      "external": "devDependencies",
      "declarations": true,
      "mappings": true
   }
}