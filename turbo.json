{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "watch": {
            "dependsOn":["^build"],
            "persistent": true,
            "cache": false
        }
    }
}